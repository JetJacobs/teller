{
	"env": {
		"es2021": true,
		"node": true
	},
	"extends": ["airbnb"],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"no-underscore-dangle": ["error", { "allow": ["_id", "__v"] }],
		"import/extensions": ["off", "ignorePackages", { "*": "always" }],
		"import/no-extraneous-dependencies": "off"
	}
}
